@using DOGEOnlineGeneralEditor.Models.ViewModels
@model MyProjectsViewModel
@{
    ViewData["Title"] = "My Projects";
}

@Scripts.Render("~/bundles/jquery")

@if (TempData["Success"] != null)
{
    <div>@TempData["Success"]</div>
}


<div class="table-header">My Projects  <a href="@Url.Action("Create", "Project")"><i class="glyphicon glyphicon-plus-sign"></i></a>  <i class="glyphicon glyphicon-search"></i> search bar birtist hér</div>

<div class="table-container">
    <table id="my-projects-table" class="table">
        @{ Html.RenderPartial("_ProjectTable", Model.MyProjects); }
    </table>
</div>

<div class="table-header">Collaboration Projects  <i class="glyphicon glyphicon-search"></i></div>

<div class="table-container">
    <table id="collaboration-projects-table" class="table">
        @{ Html.RenderPartial("_ProjectTable", Model.CollaborationProjects); }
    </table>
</div>

